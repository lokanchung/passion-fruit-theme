	"semanticTokenColors": {
		"selfKeyword": "#FFCC95",
		"property.declaration": "#FFCC95",
		"enumMember": "#fda950",
		"builtinAttribute": "#BBB",
		"*.documentation": "#595"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#595"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#FF75B5"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.name.namespace",
				"entity.name.typeAlias",
				"support.class",
				"supprtt.type.primitive"
			],
			"settings": {
				"foreground": "#fda950"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#6FC1FF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"meta.attribute",
			],
			"settings": {
				"foreground": "#bbb",
			}
		},
		{
			"name": "Preprocessor",
			"scope": "entity.name.function.preprocessor",
			"settings": {
				"foreground": "#b084eb",
			}
		},
		{
			"name": "Macros",
			"scope": [
				"entity.name.function.macro",
				"meta.macro"
			],
			"settings": {
				"foreground": "#b084eb"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#E6E6E6"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#FF75B5"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#fda950"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#45A9F9"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#FFCC95"
			}
		},
		{
			"scope": "meta.object-binding-pattern-variable.js variable",
			"settings": {
				"foreground": "#FFCC95"
			}
		},
		{
			"scope": "variable.other.object",
			"settings": {
				"foreground": "#FF9AC1"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.properties",
				"support.type.property-name",
			],
			"settings": {
				"foreground": "#FFCC95"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#19F9D8"
			}
		},
		{
			"scope": "punctuation.definition.template-expression",
